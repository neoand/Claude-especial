# Claude Code - Persistent Session State (EXAMPLE)
# Copy this to .claude.env and customize for your project
# .claude.env is git-ignored (local only)

# Current Sprint/Iteration
CURRENT_SPRINT=1
SPRINT_STATUS=not_started
SPRINT_START_DATE=YYYY-MM-DD

# Project Status
PROJECT_PHASE=planning
MAIN_FOCUS=setup

# Last Session Info
LAST_SESSION_DATE=YYYY-MM-DD
LAST_MAJOR_TASK=initial_setup

# Stats
TOTAL_SPRINTS_COMPLETED=0
TOTAL_SYNCS_WITH_TEMPLATE=0
CLAUDE_MD_VERSION=3.0

# Flags
AUTO_SYNC_ENABLED=false
THINKING_MODE_DEFAULT=true
PARALLEL_MODE_ALWAYS=true
